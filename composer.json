{
    "name": "payu/mysql-dump-anonymizer",
    "description": "Anonymizer for MySQL dumps. Can be used standalone or as a library.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "PayU\\MysqlDumpAnonymizer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayU\\MysqlDumpAnonymizer\\Tests\\": "tests/",
            "PayU\\MysqlDumpAnonymizer\\TestsIntegration\\": "tests-integration/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mnapoli/pretty": "^1.2",
        "squizlabs/php_codesniffer": "^3.7",
        "qossmic/deptrac-shim": "^0.24"
    },
    "require": {
        "php": "^7.4 || ~8.0.0 || ~8.1.0 || ~8.2.0",
        "symfony/yaml": "^5.0",
        "ext-json": "*",
        "myclabs/php-enum": "^1.7.1"
    }
}
